<%= render "adminhomepage/adminnavbar" %>
<%= form_with model: @subcategory do |form| %>
  <div class="row g-3 ">
    <div class="col-md-6">
      <div><%= form.label :category_id ,class:"form-label"%><br></div>
      <div><%= form.text_field :category_id ,:value =>@subcategory.category.id ,class:"form-control"%></div>
    </div>
    <div class="col-md-6">
      <div><%= form.label :name ,class:"form-label"%><br></div>
      <div><%= form.text_field :name ,class:"form-control"%></div>
    </div>
    <div class="col-12">
      <div class="form-label"><%=form.submit class:"btn btn-primary"%></div>
    </div>
  </div>
<% end %>

