<h1 class="text-center mt-4 mb-4">Products List</h1>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Product</th>
      <th>Price</th>
      <th>Category</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |p| %>
    <tr>
      <td><%= p.name %></td>
      <td><%= p.price %></td> 
      <td><%= p.category %></td> 
      <td><%= p.discription %></td> 
    </tr>
    <% end %>
  </tbody>
</table>
